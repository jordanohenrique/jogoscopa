<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SelecoesActivity"
    tools:ignore="MergeRootFrame"
    android:orientation="horizontal"
    android:background="@drawable/gramado">

    <FrameLayout
        android:id="@+id/frameSelJog"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/activity_15dp"
        android:layout_marginLeft="@dimen/activity_15dp"
        android:layout_marginRight="@dimen/activity_15dp"
        android:layout_marginTop="@dimen/activity_15dp"
        android:background="@android:color/white" >

    </FrameLayout>

</LinearLayout>

