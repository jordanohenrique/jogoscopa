<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!-- api 11
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="Textview" parent="AppBaseTheme"> 
        <item name="android:textColor">@color/preto</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>
    
    <style name="menuinicialtext" parent="AppBaseTheme"> 
        <item name="android:textColor">@color/branco</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>
  
    <style name="menuinicialimg" parent="AppBaseTheme"> 
        <item name="android:width">45dp</item>
        <item name="android:height">36dp</item>
    </style>    
    
    <style name="AppName" parent="AppBaseTheme">
        <item name="android:text">@string/app_name</item>
        <item name="android:textColor">@color/branco</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
    </style>

</resources>
